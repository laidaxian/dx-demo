.dialog{
	position:fixed;
	z-index:999;
	will-change: transform;
	transform: scale(0, 0);
	transition: transform 0.2s cubic-bezier(0.21, 1.29, 0.45, 1.26);
}
   
    
   
.dialog.def{
	top:0;
	left:0;
	right:0;
	bottom:0;
	margin:auto;
}

.dialog.top{
	top:0;
	margin-top:auto;
}
.dialog.bottom{
	bottom:0;
	margin-bottom:auto;
}
.dialog.left{
	left:0;
	margin-left:auto;
}
.dialog.right{
	right:0;
	margin-right:auto;
}
.dialog-mask {
	transform: scale(0, 0);
	width:100%;
	height:100%;
	left:0; 
	top:0;
	position:fixed;
	background-color:rgba(1,1,1,0.5);
	z-index:998;
}
